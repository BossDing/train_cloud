<view class="special page"><view class="questions-wrapper"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="_id"><view class="list"><view class="question">{{''+(index+1)+". "+item.title+''}}<text class="type">{{"("+typeMap[item.type]+")"}}</text></view><block wx:if="{{item.type==='3'}}"><checkbox-group data-event-opts="{{[['change',[['radioChange',['$event',index]]]]]}}" bindchange="__e"><block wx:for="{{item.items}}" wx:for-item="o" wx:for-index="__i0__" wx:key="key"><label class="item-cell"><view><checkbox value="{{o.key}}" checked="{{false}}"></checkbox></view><view>{{o.key+" 、 "+o.value}}</view></label></block></checkbox-group></block><block wx:else><radio-group data-event-opts="{{[['change',[['radioChange',['$event',index]]]]]}}" bindchange="__e"><block wx:for="{{item.items}}" wx:for-item="o" wx:for-index="__i1__" wx:key="key"><label class="item-cell"><view><radio value="{{o.key}}" checked="{{o.checked}}"></radio></view><view>{{o.key+" 、 "+o.value}}</view></label></block></radio-group></block></view></block></view><view data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" class="submit-btn" bindtap="__e">提交</view></view>