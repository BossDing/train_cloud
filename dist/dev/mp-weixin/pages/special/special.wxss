.special .questions-wrapper {
  height: 100vh;
  background: rgba(225,225,255,0.7);
}
.special .title {
  font-size: 40rpx;
  padding-top: 20rpx;
  padding-left: 20rpx;
}
.special .list {
  padding: 20rpx;
  border-bottom: 1rpx solid #c8c7cc;
}
.special .list .question {
  color: #333;
  font-size: 32rpx;
}
.special .list .item-cell {
  padding: 20rpx;
  border-bottom: 1rpx dashed #c8c7cc;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  width: 100%;
}

