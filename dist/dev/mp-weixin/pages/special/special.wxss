.special {
  height: 100vh;
  position: relative;
}
.special .questions-wrapper {
  background: rgba(225,225,255,0.7);
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 80rpx;
  overflow: auto;
}
.special .title {
  font-size: 40rpx;
  padding-top: 20rpx;
  padding-left: 20rpx;
  box-sizing: border-box;
}
.special .list {
  box-sizing: border-box;
  padding: 20rpx;
  border-bottom: 1rpx solid #c8c7cc;
}
.special .list .question {
  color: #333;
  font-size: 32rpx;
}
.special .list .item-cell {
  box-sizing: border-box;
  padding: 20rpx;
  border-bottom: 1rpx dashed #c8c7cc;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  width: 100%;
}
.special .submit-btn {
  height: 80rpx;
  width: 100vw;
  position: absolute;
  left: 0;
  bottom: 0;
  background: #4cd964;
  color: #fff;
  text-align: center;
  line-height: 80rpx;
  font-size: 36rpx;
  z-index: 1;
}

